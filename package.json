{
	"name": "zrosty-hay-app",
	"main": "expo-router/entry",
	"version": "1.0.11",
	"scripts": {
		"start": "expo start",
		"android": "expo start --android",
		"ios": "expo start --ios",
		"web": "expo start --web",
		"test": "jest --watchAll",
		"bump-patch": "npm version patch; git push --follow-tags",
		"bump-minor": "npm version minor; git push --follow-tags",
		"bump-major": "npm version major; git push --follow-tags",
		"storybook-generate": "sb-rn-get-stories",
		"sb-watch": "sb-rn-watcher",
		"sb": "STORYBOOK_ENABLED='true' expo start",
		"win-sb": "set STORYBOOK_ENABLED=true && expo start",
		"and-build-dev": "eas build -p android --non-interactive --output=builds/build-and-latest.apk --profile=development --local",
		"ios-build-dev": "eas build -p ios --non-interactive --output=builds/build-ios-latest.tar.gz --profile=development --local",
		"update-openapi-json": "./scripts/update-openapi-json.sh",
		"gen-api": "yarn orval --config ./orval.config.ts",
		"lint": "npx @biomejs/biome lint .",
		"type-check": "tsc --noEmit"
	},
	"jest": {
		"preset": "jest-expo"
	},
	"dependencies": {
		"@expo/vector-icons": "^13.0.0",
